<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- The bootstrap-context.xml file is used for patch definitions, importers, 
		workflow, and loading custom content models. -->
	<!-- Registration of new models -->
	<bean id="tutorial-repo_dictionaryBootstrap"
		parent="dictionaryModelBootstrap" depends-on="dictionaryBootstrap">
		<property name="models">
			<list>
				<value>alfresco/module/${project.artifactId}/model/civ-content-model.xml</value>
			</list>
		</property>
	</bean>
	
	<bean id="tutorial-repo_actionResourceBundles" parent="actionResourceBundles">
    <property name="resourceBundles">
        <list>
            <value>alfresco.module.${project.artifactId}.messages.civActions</value>
        </list>
    </property>
</bean>

</beans>
